<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Michał Dwojak</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="Scripts/game.js"></script>
    <link rel="stylesheet" href="~/Content/animate.css">
</head>
<body>
  
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
       

                <span>Wejdź na stronę główną: <a href="https://mdwojak.pl/">www.mdwojak.pl</a>.</span>
         
                <p>&copy; @DateTime.Now.Year - Michał Dwojak  <a href='@Url.Action("Policy", "Home")'>Polityka prywatności</a></p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
